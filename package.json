    "build": "babel source/browser-simple-loader.js --out-file build/browser-simple-loader.js && webpack --profile --display-modules --display-reasons"
  },
  "homepage": "https://github.com/alexander-usolcev/browser-simple-loader.git#readme",
  "bugs": {
